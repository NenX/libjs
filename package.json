{
    "name": "root",
    "private": true,
    "scripts": {
        "build": "lerna run build",
        "build1": "lerna exec -- rollup -c=../../rollup.config.js",
        "watch": "lerna exec --no-sort -- rollup -w -c=../../rollup.config.js",
        "clean": "lerna run clean",
        "lerna:clean": "lerna clean",
        "lint": "lerna run lint",
        "test": "lerna run test",
        "check-packages": "pnpm clean && pnpm test && pnpm lint && pnpm build",
        "precommit": "lint-staged",
        "boot": "lerna bootstrap",
        "lerna:publish": "lerna publish from-package --registry=https://registry.npmjs.org/ --legacy-auth -y",
        "release": "pnpm build && pnpm lerna:publish",
        "release:local": "./compress_packages.sh",
        "commit:release": "git add . && git commit -m 'publish'",
        "helper:serve": "pm2 start ./helper/target/release/lmfe_server",
        "helper": "./helper/target/debug/lm_fe_server"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/react": "18.3.1",
        "@types/react-dom": "18.3.1",
        "postcss-url": "^10.1.3",
        "rimraf": "^6.0.1",
        "@microsoft/api-extractor": "^7.52.2",
        "lerna": "7.4.2",
        "@babel/core": "^7.10.5",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-dynamic-import-vars": "^1.4.3",
        "@rollup/plugin-image": "^2.1.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-typescript": "^8.3.1",
        "rollup-plugin-dts": "6.2.1"
    },
    "version": "1.0.0",
    "author": "brainfuck <brainfuck@lian-med.com>",
    "license": "MIT",
    "dependencies": {
        "antd": "5.26.4",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "@babel/runtime": "^7.20.0"
    }
}